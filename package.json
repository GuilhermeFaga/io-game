{
  "name": "io-game-v2",
  "version": "0.1.6",
  "description": "Networking with sockets.",
  "main": "./app/main.js",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "standard": "^14.3.1",
    "webpack": "~4.41.0"
  },
  "scripts": {
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valkyrienyanko/io-game-v2.git"
  },
  "keywords": [
    "socket.io",
    "p5.js"
  ],
  "author": "valkyrienyanko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valkyrienyanko/io-game-v2/issues"
  },
  "homepage": "https://github.com/valkyrienyanko/io-game-v2#readme",
  "build": {
    "appId": "me.valk.iogame",
    "productName": "IO Game",
    "target": "NSIS",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "build/installerIcon.ico",
      "installerHeaderIcon": "build/installerHeaderIcon.ico",
      "uninstallerIcon": "build/uninstallerIcon.ico",
      "uninstallDisplayName": "${productName} ${version}",
      "artifactName": "${productName} ${version} Setup.${ext}",
      "deleteAppDataOnUninstall": true,
      "runAfterFinish": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "menuCategory": false,
      "shortcutName": "${productName}"
    }
  }
}
